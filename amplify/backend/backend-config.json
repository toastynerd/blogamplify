{
  "api": {
    "toastyblog": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 7,
              "description": "toasty-blog"
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    },
    "AdminQueries": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "toastyblog",
          "attributes": [
            "UserPoolId"
          ]
        },
        {
          "category": "function",
          "resourceName": "AdminQueriesb85d3e0c",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ]
    }
  },
  "function": {
    "toastyblogCreateAuthChallenge": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "toastyblogDefineAuthChallenge": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "toastyblogVerifyAuthChallengeResponse": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "AdminQueriesb85d3e0c": {
      "service": "Lambda",
      "providerPlugin": "awscloudformation",
      "build": true,
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "toastyblog",
          "attributes": [
            "UserPoolId"
          ]
        }
      ]
    }
  },
  "auth": {
    "userPoolGroups": {
      "service": "Cognito-UserPool-Groups",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "toastyblog",
          "attributes": [
            "UserPoolId",
            "AppClientIDWeb",
            "AppClientID"
          ]
        }
      ]
    },
    "toastyblog": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "toastyblogCreateAuthChallenge",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "toastyblogDefineAuthChallenge",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "toastyblogVerifyAuthChallengeResponse",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ],
      "customAuth": true
    }
  }
}